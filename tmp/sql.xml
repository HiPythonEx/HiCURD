<?xml version="1.0"?>
<SQL-List>
<MODULE id="SQL" script="#cname#">
	<SQL id="#remark#.List" type="sqlserver">
		<TEXT>
		 Search#table#
		</TEXT>
		<Paramers>
#Search#
		</Paramers>
	</SQL>
	<SQL id="#remark#.Save" type="sqlserver">
		<TEXT>
		 Save#table#
		</TEXT>
		<Paramers>
#Save#
		</Paramers>
	</SQL>
	<SQL id="#remark#.Delete" type="sqlserver">
		<TEXT>
		 Delete#table#
		</TEXT>
		<Paramers>
#condition_params#
		</Paramers>
	</SQL>
	<SQL id="#remark#.Get" type="sqlserver">
		<TEXT>
		SELECT  * FROM [#table#] WHERE 1=1 #condition#
		</TEXT>
		<Paramers>
#condition_params#
		</Paramers>
	</SQL>
</MODULE>
</SQL-List>

